{
    "name": "baytek/laravel-content-discussion",
    "description": "Discussion Content Provider",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Yvon Viger",
            "email": "yvon@baytek.ca"
        }
    ],
    "autoload": {
        "psr-4": {
            "Baytek\\Laravel\\Content\\Types\\Discussion\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "baytek/laravel-content": "~1.2.0"
    },
    "post-install-cmd": [
        "php artisan install:discussion"
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Baytek\\Laravel\\Content\\Types\\Discussion\\DiscussionContentServiceProvider"
            ]
        }
    }
}
